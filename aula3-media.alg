algoritmo "Valores"
// Função :
// Autor :
// Data : 17/09/2023
// Seção de Declarações 
var
    nota1, nota2: inteiro
    media: real
inicio
      escreva("Primeira nota: ")
      leia(nota1)
      escreva("Segunda nota: ")
      leia(nota2)
      media <- (nota1 + nota2) /2
      Escreval("Sua media é " , media)
fimalgoritmo
