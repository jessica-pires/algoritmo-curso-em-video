algoritmo "semnome"
// Função :
// Autor :
// Data : 19/09/2023
// Seção de Declarações 
var
 contador,N, Soma: inteiro
inicio
     contador <-1
     Soma <-0
     Enquanto(contador <= 10)faca
     Escreva("Digite um valor: ")
     Leia(N)
     Soma<- Soma +N
     contador <- contador+1
     Fimenquanto
     Escreval("A soma de todos os valores foi ", Soma)
fimalgoritmo
