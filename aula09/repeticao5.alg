algoritmo "contagemInteligente"

var
 valor1, fim ,CONTADOR:inteiro
inicio
    Escreval("CONTAGEM INTELIGENTE")
    Escreval("=====================")
    Escreva("INICIO: ")
    Leia(valor1)
    Escreva("FIM: ")
    Leia(fim)
    Enquanto(valor1 <= fim)faca
      Escreva(CONTADOR,"...")
      CONTADOR<- CONTADOR +1
    fimenquanto
fimalgoritmo
